<?import javafx.scene.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox alignment="CENTER" fillHeight="false" prefHeight="50.0" spacing="20.0">
         <children>
            <Button fx:id="button1" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#LabelIncreaser" text="Ofra Haza">
               <HBox.margin>
                  <Insets bottom="100.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="button2" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#LabelDecreaser" text="Yardena Arazi">
               <HBox.margin>
                  <Insets bottom="100.0" />
               </HBox.margin>
            </Button>
         </children>
         <padding>
            <Insets right="10.0" top="10.0" />
         </padding>
      </HBox>
      <Label fx:id="label" alignment="CENTER" maxWidth="1.7976931348623157E308" prefHeight="50.0" style="-fx-background-color: red;" text="0">
         <VBox.margin>
            <Insets bottom="20.0" />
         </VBox.margin>
      </Label>
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
